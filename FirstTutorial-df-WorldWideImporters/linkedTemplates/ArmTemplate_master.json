{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "FirstTutorial-df-WorldWideImporters"
		},
		"AzureDataLakeStorageLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLS'"
		},
		"AzureDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureDatabaseLS'"
		},
		"LS_ADLS_FirstTutorial_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_FirstTutorial'"
		},
		"LS_OnPrem_FS_Temp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_OnPrem_FS_Temp'"
		},
		"LS_OnPrem_sqldb_dev_pollution_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_OnPrem_sqldb_dev_pollution'"
		},
		"LS_SQLDB_FirstTutorial_sqldb_WideWorldImporters_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB_FirstTutorial_sqldb_WideWorldImporters'"
		},
		"LS_SQLDB_ONPREM_WorldWideImportersDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB_ONPREM_WorldWideImportersDW'"
		},
		"OnPremFileSystemLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremFileSystemLS'"
		},
		"OnPremiseSQLServerLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremiseSQLServerLS'"
		},
		"AzureDataLakeStorageLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firsttutorialdatastorev2.dfs.core.windows.net"
		},
		"LS_ADLS_FirstTutorial_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firsttutorialdatastorev2.dfs.core.windows.net"
		},
		"LS_OnPrem_FS_Temp_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "E:\\Temp"
		},
		"LS_OnPrem_FS_Temp_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "AzureUser"
		},
		"OnPremFileSystemLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "E:\\FileSystemOnPrem"
		},
		"OnPremFileSystemLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "dev\\chris"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLS_accountKey')]"
					},
					"AzureDatabaseLS_connectionString": {
						"value": "[parameters('AzureDatabaseLS_connectionString')]"
					},
					"LS_ADLS_FirstTutorial_accountKey": {
						"value": "[parameters('LS_ADLS_FirstTutorial_accountKey')]"
					},
					"LS_SQLDB_FirstTutorial_sqldb_WideWorldImporters_connectionString": {
						"value": "[parameters('LS_SQLDB_FirstTutorial_sqldb_WideWorldImporters_connectionString')]"
					},
					"LS_OnPrem_FS_Temp_password": {
						"value": "[parameters('LS_OnPrem_FS_Temp_password')]"
					},
					"LS_OnPrem_sqldb_dev_pollution_connectionString": {
						"value": "[parameters('LS_OnPrem_sqldb_dev_pollution_connectionString')]"
					},
					"LS_SQLDB_ONPREM_WorldWideImportersDW_connectionString": {
						"value": "[parameters('LS_SQLDB_ONPREM_WorldWideImportersDW_connectionString')]"
					},
					"OnPremFileSystemLS_password": {
						"value": "[parameters('OnPremFileSystemLS_password')]"
					},
					"OnPremiseSQLServerLS_connectionString": {
						"value": "[parameters('OnPremiseSQLServerLS_connectionString')]"
					},
					"AzureDataLakeStorageLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLS_properties_typeProperties_url')]"
					},
					"LS_ADLS_FirstTutorial_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_FirstTutorial_properties_typeProperties_url')]"
					},
					"LS_OnPrem_FS_Temp_properties_typeProperties_host": {
						"value": "[parameters('LS_OnPrem_FS_Temp_properties_typeProperties_host')]"
					},
					"LS_OnPrem_FS_Temp_properties_typeProperties_userId": {
						"value": "[parameters('LS_OnPrem_FS_Temp_properties_typeProperties_userId')]"
					},
					"OnPremFileSystemLS_properties_typeProperties_host": {
						"value": "[parameters('OnPremFileSystemLS_properties_typeProperties_host')]"
					},
					"OnPremFileSystemLS_properties_typeProperties_userId": {
						"value": "[parameters('OnPremFileSystemLS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}